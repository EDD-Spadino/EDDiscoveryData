  "Programs": {
    "ProgramSet": [
      {
        "Name": "onStartup",
        "Steps": [
          {
            "StepName": "Call",
            "StepUC": "SetUpLanguage",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "If",
            "StepUC": "%version(1) < 6",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Say",
            "StepUC": "\"Warning This voice pack requires version 6 of elite dangerous discovery at a minimum.\", Volume=100",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Else",
            "StepUC": "",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Say",
            "StepUC": "%(say_welcome)",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Print",
            "StepUC": "Voice Pack installed, running on EDD Version %version(1).%version(2).%version(3).%version(4)",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Call",
            "StepUC": "Initialise",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          }
        ]
      },
      {
        "Name": "SetUpLanguage",
        "Steps": [
          {
            "StepName": "rem",
            "StepUC": "do these.. If %(CurrentCulture) Contains sv",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "call",
            "StepUC": "SetUpLanguageEnglish",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          }
        ]
      },
      {
        "Name": "onRefreshEnd",
        "Steps": [
          {
            "StepName": "If",
            "StepUC": "%(RefreshCount) <= 1",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Event",
            "StepUC": "PREFIX LAST_ LAST",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Ledger",
            "StepUC": "PREFIX LED_ AtOrBefore %(LAST_JID)",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Event",
            "StepUC": "PREFIX RK_ LAST Rank",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Set",
            "StepUC": "speech = \"%phrase(\\\"%expand(say_refresh)\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "speech = \"%replacevar(speech,\\\"say_tx\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Say",
            "StepUC": "%(speech), Literal =1 ",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          }
        ]
      },
      {
        "Name": "GenericResponse",
        "Steps": [
          {
            "StepName": "Set",
            "StepUC": "v = say_%(EventJS_event)",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Set",
            "StepUC": "prior = \"%existsdefault(Priority,\\\"Normal\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Set",
            "StepUC": "speech = \"%phrase(\\\"%indirect(v)\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "speech = \"%replacevar(speech,\\\"say_tx\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 1
          },
          {
            "StepName": "If",
            "StepUC": "HighVolume IsPresent",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Say",
            "StepUC": "%(speech), Volume=100, Priority=%(prior), Literal = 1",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Else",
            "StepUC": "",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Say",
            "StepUC": "%(speech), Priority=%(prior), Literal = 1",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          }
        ]
