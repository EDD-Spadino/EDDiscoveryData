      {
        "Name": "SaySystemInfo",
        "Steps": [
          {
            "StepName": "REM",
            "StepUC": "Print File Version SaySysInfo %(ScanSystem) %(TriggerName)",
            "StepLevelUp": 0,
            "StepWhitespace": 1
          },
          {
            "StepName": "If",
            "StepUC": "TriggerName $== onSayFinished",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Timer",
            "StepUC": "ScanDelay,\"%eval(\\\"%random(10)*1000+2000\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "End",
            "StepUC": "",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Else",
            "StepUC": "",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Scan",
            "StepUC": "EDSM  \"%(ScanSystem)\"",
            "StepLevelUp": 0,
            "StepWhitespace": 1
          },
          {
            "StepName": "set",
            "StepUC": "speech = \"\"",
            "StepLevelUp": 0,
            "StepWhitespace": 1
          },
          {
            "StepName": "if",
            "StepUC": "%(SpeechVerbosity) >= 2 And %(S_Stars) == 0",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "speech += \"%phrase(\\\"%expand(say_edsmscannoinfo)\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 1
          },
          {
            "StepName": "Loop",
            "StepUC": "%(S_Stars)",
            "StepLevelUp": 1,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "prefix = S_Star_%(Loop2)_",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "planets = \"%indirect(\\\"%(prefix)Planets\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "data = \"%indirect(\\\"%(prefix)data\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "type = \"%indirect(\\\"%(prefix)type\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "ownname = \"%indirect(\\\"%(prefix)assignedname\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 1
          },
          {
            "StepName": "if",
            "StepUC": "type $== barycentre",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "speech += %expand(say_edsmscanstarbarycentre)",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "elseif",
            "StepUC": "data IsTrue",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "name = \"%indirect(\\\"%(prefix)bodyname\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "startype = \"%indirect(\\\"%(prefix)startypetext\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "mass = \"%indirect(\\\"%(prefix)stellarmass\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "speech += %expand(say_edsmscanstarinfo)",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "else",
            "StepUC": "",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "speech += %expand(say_edsmscanstarnoinfo)",
            "StepLevelUp": 0,
            "StepWhitespace": 1
          },
          {
            "StepName": "Loop",
            "StepUC": "%(planets)",
            "StepLevelUp": 1,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "prefix = S_Planet_%(Loop2)_%(Loop3)_",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "call",
            "StepUC": "saySystemInfoBody(prefix=\"%(prefix)\",S_*$=1)",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "speech += %(ReturnValue)",
            "StepLevelUp": 0,
            "StepWhitespace": 1
          },
          {
            "StepName": "if",
            "StepUC": "%(SpeechVerbosity) >= 3",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "moons = \"%indirect(\\\"%(prefix)Moons\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 1
          },
          {
            "StepName": "Loop",
            "StepUC": "%(moons)",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "prefix = S_Moon_%(Loop2)_%(Loop3)_%(Loop5)_",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "call",
            "StepUC": "saySystemInfoBody(prefix=\"%(prefix)\",S_*$=1)",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "speech += %(ReturnValue)",
            "StepLevelUp": 0,
            "StepWhitespace": 1
          },
          {
            "StepName": "if",
            "StepUC": "%(SpeechVerbosity) >= 4",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "submoons = \"%indirect(\\\"%(prefix)Submoons\\\")\"",
            "StepLevelUp": 0,
            "StepWhitespace": 1
          },
          {
            "StepName": "Loop",
            "StepUC": "%(submoons)",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "prefix = S_SubMoon_%(Loop2)_%(Loop3)_%(Loop5)_%(Loop7)_",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "call",
            "StepUC": "saySystemInfoBody(prefix=\"%(prefix)\",S_*$=1)",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "set",
            "StepUC": "speech += %(ReturnValue)",
            "StepLevelUp": 0,
            "StepWhitespace": 1
          },
          {
            "StepName": "set",
            "StepUC": "speech = \"%replacevar(speech,\\\"say_tx\\\")\"",
            "StepLevelUp": 6,
            "StepWhitespace": 0
          },
          {
            "StepName": "say",
            "StepUC": "%(speech),Priority=Low,Literal=1",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          }
        ]
