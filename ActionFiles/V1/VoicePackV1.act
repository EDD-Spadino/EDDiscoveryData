  "Programs": {
    "ProgramSet": [
      {
        "Name": "onStartup",
        "Steps": [
          {
            "StepName": "Call",
            "StepUC": "SetUpLanguage",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "If",
            "StepUC": "%version(1) < 5",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Say",
            "StepUC": "\"Warning This voice pack requires version 5 of elite dangerous discovery at a minimum.\", Volume=100",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Else",
            "StepUC": "",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Say",
            "StepUC": "%(say_welcome)",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Print",
            "StepUC": "Voice Pack installed, running on EDD Version %version(1).%version(2).%version(3).%version(4)",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          }
        ]
      },
      {
        "Name": "SetUpLanguage",
        "Steps": [
          {
            "StepName": "rem",
            "StepUC": "do these.. If %(CurrentCulture) Contains sv",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "call",
            "StepUC": "SetUpLanguageEnglish",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          }
        ]
      },
      {
        "Name": "onRefreshEnd",
        "Steps": [
          {
            "StepName": "If",
            "StepUC": "%(RefreshCount) <= 1",
            "StepLevelUp": 0,
            "StepWhitespace": 0
