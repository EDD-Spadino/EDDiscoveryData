          {
            "StepName": "Event",
            "StepUC": "PREFIX LAST_ LAST",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Ledger",
            "StepUC": "PREFIX LED_ AtOrBefore %(LAST_JID)",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Event",
            "StepUC": "PREFIX RK_ LAST Rank",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Say",
            "StepUC": "%expand(say_refresh)",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          }
        ]
      },
      {
        "Name": "GenericResponse",
        "Steps": [
          {
            "StepName": "Set",
