    "ProgramSet": [
      {
        "Name": "TogglePopout",
        "Steps": [
          {
            "StepName": "if",
            "StepUC": "KeyPanel NotPresent Or KeyPanel $== Complete",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Popout",
            "StepUC": "%(panel)1 status",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "If",
            "StepUC": "%(P_Exists) IsTrue",
            "StepLevelUp": 0,
            "StepWhitespace": 0
          },
          {
            "StepName": "Popout",
